{
  "name": "@cognitive3d/analytics",
  "version": "2.0.0",
  "description": "This SDK allows you to integrate your Javascript Applications with Cognitive3D, which provides analytics and insights about your VR/AR/MR project.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "browser": "./lib/index.umd.js"
    },
    "./adapters/threejs": {
      "import": "./lib/esm/adapters/threejs-adapter.js",
      "require": "./lib/cjs/adapters/threejs-adapter.js"
    },
    "./adapters/babylon": {
      "import": "./lib/esm/adapters/babylon-adapter.js",
      "require": "./lib/cjs/adapters/babylon-adapter.js"
    },
    "./adapters/playcanvas": {
      "import": "./lib/esm/adapters/playcanvas-adapter.js",
      "require": "./lib/cjs/adapters/playcanvas-adapter.js"
    }
  },
  "files": [
    "lib",
    "types"
  ],
  "types": "types/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "test": "jest",
    "test:browser": "jest --config jest.browser.config.js",
    "test:node": "jest --config jest.node.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CognitiveVR/cvr-sdk-js.git"
  },
  "keywords": [
    "analytics",
    "vr",
    "ar",
    "xr",
    "cognitive3d"
  ],
  "author": "Cognitive3D support@cognitive3d.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CognitiveVR/c3d-sdk-webxr/issues"
  },
  "homepage": "https://github.com/CognitiveVR/c3d-sdk-webxr#readme",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.43.0"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "three": ">=0.128.0",
    "babylonjs": ">=5.0.0",
    "playcanvas": ">=1.50.0"
  },
  "peerDependenciesMeta": {
    "three": {
      "optional": true
    },
    "babylonjs": {
      "optional": true
    },
    "playcanvas": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}